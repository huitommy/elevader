<h1>Hello!</h1>

<% @elevators.each do |elevator| %>
  <div class="elevator" id="elevator-<%= elevator.id %>">
    <%= link_to "#{elevator.building_name}", elevator_path(elevator) %>
    <% if current_admin || current_user == elevator.user %>
      <%= button_to "Delete", elevator, method: :delete %>
    <% end %>
  </div>
<% end %>

<%= link_to "Add Elevator", new_elevator_path %>
