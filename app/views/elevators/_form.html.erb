<%= form_for(@elevator) do |f| %>
  <div class= "field">
    <%= f.label :building_name %>
    <%= f.text_field :building_name %>
  </div>

  <div class= "field">
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div class= "field">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div class= "field">
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>
  </div>

  <div class= "field">
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div class= "actions">
    <%= f.submit%>
  </div>
<% end %>
